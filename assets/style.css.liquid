}

body.offside-js--is-open{
  overflow: hidden;
}

.offside-js--is-open .offside {
    -webkit-box-shadow: -4px 0px 40px 1px rgba(0,0,0,0.75);
    -moz-box-shadow: -4px 0px 40px 1px rgba(0,0,0,0.75);
    box-shadow: -4px 0px 40px 1px rgba(0,0,0,0.75);
}

.offside {
    display: none;
}

.offside-js--init .offside {
    display: block;
}

#slideInMenu.offside {
  overflow: hidden;
    background: white;
    width: 100%;
    z-index: 20000001;
}

#slideInMenu.offside--right {
    right: -100%;
}

#slideInMenu.offside--left {
    left: -100%;
}

#slideInMenu.offside--right.is-open,
#slideInMenu.offside-js--is-right .offside-sliding-element {
    -webkit-transform: translate3d(-100%, 0, 0);
    -moz-transform: translate3d(-100%, 0, 0);
    -ms-transform: translate3d(-100%, 0, 0);
    -o-transform: translate3d(-100%, 0, 0);
    transform: translate3d(-100%, 0, 0);
}

#slideInMenu .slideInMenuInner {
  position: relative;
  height: 100%;
  width: 100%;
}

{% assign slideInMenuWidth = '500px' %}
@media(min-width: 680px){
  
  #slideInMenu.offside {
  overflow: hidden;
    background: white;
    width: {{ slideInMenuWidth }};
    z-index: 20000001;
  }

  #slideInMenu.offside--right {
      right: -{{ slideInMenuWidth }};
  }

  #slideInMenu.offside--left {
      left: -{{ slideInMenuWidth }};
  }

  #slideInMenu.offside--right.is-open,
  #slideInMenu.offside-js--is-right .offside-sliding-element {
      -webkit-transform: translate3d(-{{ slideInMenuWidth }}, 0, 0);
      -moz-transform: translate3d(-{{ slideInMenuWidth }}, 0, 0);
      -ms-transform: translate3d(-{{ slideInMenuWidth }}, 0, 0);
      -o-transform: translate3d(-{{ slideInMenuWidth }}, 0, 0);
      transform: translate3d(-{{ slideInMenuWidth }}, 0, 0);
  }
}

/* mobile ajax cart fixes */
#CartContainer{.
  position: relative;
  overflow: auto;
  height: calc(100% - 72px);
}

@media (max-width: 1024px){
  #CartContainer{
    overflow-y: scroll !important;
