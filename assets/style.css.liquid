  font-size: 30px;
  line-height: 1;
  margin-top: -15px;
  margin-left: -15px;
  pointer-events: none;
  visibility: hidden;
  color: #fff;
  opacity: 0;
  transition: all 0.2s ease-in-out;
}
.product-overlay .po-spinner.show-spinner {
    visibility:visible;
    opacity:1;
}
.product-overlay .po-spinner i {
    animation: po-spinner 1s ease-in-out infinite;
}
.product-overlay .po-qty {
    position: absolute;
    top: 50%;
    left: 0;
    width: 100%;
    text-align: center;
    margin-top: -12px;
    font-size: 24px;
    color: #fff;
    line-height: 1;
    pointer-events:none;
    visibility: hidden;
    opacity:0;
    transition:all 0.2s ease-in-out;
}
.product-overlay .po-qty.show-qty {
    visibility: visible;
    opacity:1;
}
@keyframes po-spinner {
  0% {
    transform: rotate(0);
  }
  100% {
    transform: rotate(360deg); 
  }
}

